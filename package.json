{
  "name": "robotum-frontend",
  "version": "1.0.0",
  "description": "Frontend codebase for RoboTUM â€” a student-led robotics club. Built with React + Tailwind CSS. Designed to be fast, modular, and easy to scale.",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:clean": "npm run build && npm run css:purge",
    "css:purge": "purgecss --css 'dist/assets/*.css' --content 'index.html' 'src/**/*.{js,jsx}' --safelist 'html,body,#root,nav,section,header,footer' --output dist/assets",
    "css:report": "purgecss --css 'dist/assets/*.css' --content 'index.html' 'src/**/*.{js,jsx}' --rejected --output dist/purged && echo 'See dist/purged for purged CSS; rejected selectors are printed above.'"
    
  },
